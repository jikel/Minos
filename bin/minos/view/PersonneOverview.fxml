                     </columnResizePolicy>
                  </TableView>
               </children></AnchorPane>
        </content>
      </TitledPane>
      <Button layoutX="14.0" layoutY="512.0" mnemonicParsing="false" onAction="#ajoutRequete" text="Nouvelle requete" />
