<AnchorPane prefHeight="800.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="minos.controller.PersonneOverviewController">
   <children>
      <TitledPane animated="false" layoutX="14.0" layoutY="14.0" text="Personne" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0">
