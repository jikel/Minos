<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="200.0" prefWidth="500.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="minos.controller.requeteoverview.ModifierRequeteController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Modifier la requête">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <GridPane layoutX="14.0" layoutY="44.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Numéro d'auditorat" />
            <TextField fx:id="numeroAuditorat" GridPane.columnIndex="1" />
            <TextField fx:id="numeroRG" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label text="Numéro RG" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <Button fx:id="btnModifier" layoutX="333.0" layoutY="138.0" mnemonicParsing="false" onAction="#modifierRequete" text="Modifier" />
      <Button fx:id="btnAnnuler" layoutX="414.0" layoutY="138.0" mnemonicParsing="false" onAction="#annuler" text="Annuler" />
   </children>
</AnchorPane>
