<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="minos.controller.InfoRequeteController">
   <children>
      <GridPane layoutX="14.0" layoutY="14.0" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Numéro d'auditorat" />
            <Label fx:id="numeroAuditorat" text="Label" GridPane.columnIndex="1" />
            <Label text="Numéro de role" GridPane.rowIndex="1" />
            <Label fx:id="numeroRole" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label text="Date de création" GridPane.rowIndex="2" />
            <Label fx:id="dateCreation" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Label text="Etat" GridPane.rowIndex="3" />
            <Label fx:id="etat" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Label text="Date jugement" GridPane.rowIndex="6" />
            <Label fx:id="dateJugement" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="6" />
            <Label text="Dispositif" GridPane.rowIndex="7" />
            <Label fx:id="dispositif" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="7" />
            <Hyperlink onAction="#modifAuditorat" text="Modifier" GridPane.columnIndex="2" />
            <Hyperlink onAction="#modifRole" text="Modifier" GridPane.columnIndex="2" GridPane.rowIndex="1" />
            <Hyperlink onAction="#modifEtat" text="Modifier" GridPane.columnIndex="2" GridPane.rowIndex="3" />
            <Hyperlink onAction="#modifDispositif" text="Modifier" GridPane.columnIndex="2" GridPane.rowIndex="7" />
            <DatePicker onAction="#modifDateJugement" GridPane.columnIndex="2" GridPane.rowIndex="6" />
            <Label text="Avocat" GridPane.rowIndex="4" />
            <Label fx:id="avocat" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <Hyperlink onAction="#modifAvocat" text="Modifier" GridPane.columnIndex="2" GridPane.rowIndex="4" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
